<h1>Log In</h1>

<form action="<%=session_url%>" method="post">
  
  <label for="username">username</label>
  <input
    type="text"
    name="user[username]"
    id="username"
    value="<%= @user.username %>">

  <label for="password">password</label>
  <input
    type="password"
    name="user[password]"
    id="password"
    value="<%= @user.password %>">

  <input type="submit" value="log in......">
</form>